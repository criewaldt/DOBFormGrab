<div class="span9 about-main-image">
<div class="span6 about-description"><h1>Raising or Moving a Building Notification Form</h1>
<script type="text/javascript" src="/assets/home/js/forms/forms-v3.js"></script>
<script type="text/javascript" src="/assets/buildings/js/forms/forms-online-rmb.js"></script>
<form class="control-form" action="https://apps.nyc.gov/nyc-mailform/validation" name="Forms Online RMB" method="post" id="forms-online-rmb" style="margin:0;">
	<input type="hidden" value="Forms Online RMB" name="subject" />
	<input type="hidden" value="REMOTE_HOST,HTTP_ADDR,HTTP_USER_AGENT" name="env_report" />
	<input type="hidden" value="#FFFFFF" name="bgcolor" />
	<input value="Borough,Street Number,Street Name,Job Number,Permit Holder Name,Special Inspector Name,Special Inspector License Number,Special Inspector Phone,Special Inspector Email,Emergency Phone,Date of First Raise,Date of Structure Reattachment" name="required" id="required" type="hidden" />
        <input type="hidden" value="3" name="g-recaptcha-version">
	<div class="row">
		<div class="container">
			<div class="span12">
				<p><span class="required">*</span> <strong>Indicates required fields</strong></p>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<h2>Building Information</h2>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Borough<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<select name="Borough" aria-required="true" >
							<option value=""></option>
							<option value="Bronx">Bronx</option>
							<option value="Brooklyn">Brooklyn</option>
							<option value="Manhattan">Manhattan</option>
							<option value="Queens">Queens</option>
							<option value="Staten Island">Staten Island</option>
						</select>
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Street Number<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Street Number" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Street Name<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Street Name" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<hr>
				<h2>Job Information</h2>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Job Number<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Job Number" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Permit Holder Name<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Permit Holder Name" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Special Inspector Name<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Special Inspector Name" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Special Inspector License Number<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="text" size="40" name="Special Inspector License Number" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Special Inspector Phone<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="tel" size="40" name="Special Inspector Phone" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Special Inspector Email<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="email" size="40" name="Special Inspector Email" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label">Emergency Phone<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" type="tel" size="40" name="Emergency Phone" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<hr>
				<h2>Job Date Information</h2>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label" for="date-of-first-raise">Date of First Raise<span class="required">*</span><span class="validationMessage" aria-live="polite" style="display: none" aria-hidden="true"></span>
						<input aria-required="true" id="date-of-first-raise" placeholder="Select a Date" name="Date of First Raise" class="agency-datepicker filter-item" type="text" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
			<div class="span12">
				<div class="control-group">
					<label class="control-label" for="date-of-lateral-movement">Date of Lateral Movement
						<input id="date-of-lateral-movement" placeholder="Select a Date" name="Date of Lateral Movement" class="agency-datepicker filter-item" type="text" />
					</label>
				</div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="container">
